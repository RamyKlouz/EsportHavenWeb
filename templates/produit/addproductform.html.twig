{% extends 'header.oc.html.twig' %}

{% block title %}Ajouter Produit{% endblock %}

{% block body %}
<h1>Ajouter Produit</h1>

{# {{ form(f) }} #}
    {{ form_start(f) }}
    <table>
    <tr>
    <td>{{ form_label(f.nom,"Nom du Produit :") }}</td>
        <td> {{ form_widget(f.nom) }}</td>
        <td> {{ form_errors(f.nom) }}</td>
    </tr><tr>
    <td>  {{ form_label(f.type, "Type du Produit :")}}</td>
    <td>  {{ form_widget(f.type) }}</td>
    <td>  {{ form_errors(f.type) }}</td>
    </tr><tr>
    <td>  {{ form_label(f.marque, "Marque du Produit :")}}</td>
    <td>  {{ form_widget(f.marque) }}</td>
    <td>  {{ form_errors(f.marque) }}</td>
    </tr>
    <td>  {{ form_label(f.quantite, "Quantit√© :")}}</td>
    <td>  {{ form_widget(f.quantite) }}</td>
    <td>  {{ form_errors(f.quantite) }}</td>
    </tr>
    <tr>
        <td>  {{ form_label(f.prix, "Prix :" )}}</td>
        <td>  {{ form_widget(f.prix) }}</td>
        <td>  {{ form_errors(f.prix) }}</td>
    </tr></table>
    {{  form_end(f) }}
    <a href="{{ path('Aff') }}">Liste des Produits</a>
{% endblock %}
